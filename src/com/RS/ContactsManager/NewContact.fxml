<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.RS.ContactsManager.NewContactController">
    <content>
        <GridPane>
            <Label text="First Name" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="firstNameField" promptText="Enter First Name" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Last Name" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextField fx:id="lastNameField" promptText="Enter Last Name" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Phone Number" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <TextField fx:id="phoneNumberField" promptText="Enter Phone Number" GridPane.rowIndex="2" GridPane.columnIndex="1"/>


        </GridPane>
    </content>

</DialogPane>
